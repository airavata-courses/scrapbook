import { NgxsPlugin } from '@ngxs/store';
import { ResetService } from './reset.service';
export declare class NgxsResetPlugin implements NgxsPlugin {
    private readonly resetService;
    constructor(resetService: ResetService);
    private clearStates;
    private overwriteStates;
    private resetStates;
    private resetStatesAll;
    handle(state: any, action: any, next: any): any;
}
